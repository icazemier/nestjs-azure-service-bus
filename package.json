{
  "name": "@icazemier/nestjs-azure-service-bus",
  "version": "0.0.17",
  "homepage": "https://github.com/icazemier/nestjs-azure-service-bus",
  "main": "index.js",
  "types": "index.d.ts",
  "description": "A dynamic module for NestJS that provides integration with Azure Service Bus.",
  "author": "engcfraposo",
  "contributors": [
    {
      "name": "I. Cazemier",
      "email": "git@warmemelk.nl",
      "url": "https://github.com/icazemier"
    }
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@azure/identity": "^4.9.1",
    "@azure/service-bus": "^7.9.5",
    "@nestjs/common": "^10.4.17",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.17",
    "@nestjs/platform-express": "^10.4.17",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.8",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
